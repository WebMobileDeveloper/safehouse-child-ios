# platform :ios, '9.0'

target 'SafehouseChild' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Safehouse
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  pod 'Firebase/Messaging'
  pod 'FirebaseInstanceID'
  #Facebook
  pod 'FBSDKLoginKit'
  
  
  pod 'SwiftKeychainWrapper'
  pod 'Alamofire'
  
  target 'SafehouseChildTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SafehouseChildUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
